<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gifty.hci.gifty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/GiftyTheme">
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchFriendsActivity" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile" />
        <activity
            android:name=".EditProfileActivity"
            android:label="@string/title_activity_edit_profile" />
        <activity
            android:name=".WishlistActivity"
            android:label="@string/title_activity_wishlist" />
        <activity
            android:name=".MyProfileWishlistActivity"
            android:label="@string/title_activity_my_profile_wishlist" />
        <activity
            android:name=".CreateWishlistActivity"
            android:label="@string/title_activity_create_wishlist" />
        <activity
            android:name=".ChooseWishlistActivity"
            android:label="@string/title_activity_choose_wishlist" />
        <activity
            android:name=".BaseActivity"
            android:label="@string/title_activity_base"></activity>
    </application>

</manifest>